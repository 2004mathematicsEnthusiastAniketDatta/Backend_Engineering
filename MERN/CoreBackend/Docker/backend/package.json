    "prisma": "^6.14.0",
    "undici": "^7.15.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
